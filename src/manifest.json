{
  "name": "Contributors on Github",
  "description": "Show Contributor Stats on Github",
  "short_name": "Stats on Github",
  "homepage_url": "https://github.com/hzoo/contributors-on-github",
  "key": "",
  "version": "0.0.7",
  "icons": {
    "128": "icon-128.png"
  },
  "permissions": [
    "https://github.com/*/*",
    "storage",
    "identity",
    "contextMenus",
    "activeTab"
  ],
  "optional_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "persistent": false,
    "scripts": [
      "vendor/webext-domain-permission-toggle.js",
      "vendor/webext-dynamic-content-scripts.js",
      "background.js"
    ]
  },
  "manifest_version": 2,
  "minimum_chrome_version": "49",
  "content_scripts": [
    {
      "run_at": "document_start",
      "matches": [
        "https://github.com/*/*"
      ],
      "css": [
        "content.css"
      ],
      "js": [
        "storage.js",
        "vendor/github-injection.js",
        "vendor/sprint.min.js",
        "content.js"
      ]
    }
  ],
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  }
}
